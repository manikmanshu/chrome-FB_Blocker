{
  "manifest_version": 2,
  "name": "My facebook blocker",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-2.2.2.min.js","content.js"],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }

}